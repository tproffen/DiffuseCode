cython discuspy.pyx
gcc -pthread -fno-strict-aliasing -DNDEBUG -g -fwrapv -O2 -Wall -Wstrict-prototypes -fPIC -I/usr/include/python2.7 -c discuspy.c -o discuspy.o
gfortran -fPIC -c discus_wrap.f90
gcc -pthread -shared -Wl,-O1 -Wl,-Bsymbolic-functions -Wl,-Bsymbolic-functions -Wl,-z,relro -fno-strict-aliasing -DNDEBUG -g -fwrapv -O2 -Wall -Wstrict-prototypes -D_FORTIFY_SOURCE=2 -g -fstack-protector --param=ssp-buffer-size=4 -Wformat -Werror=format-security discus_wrap.o discuspy.o CMakeFiles/discus_all.dir/*.o ../../lib_f90/liblib_f90.a ../../lib_f90/liblib_f90c.a -lgfortran -lreadline -o discuspy.so
